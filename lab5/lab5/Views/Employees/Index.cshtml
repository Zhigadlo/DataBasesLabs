@{
    ViewData["Title"] = "Employee list";
}
@using lab5.Data.Models
@model IEnumerable<Employee>
<div>
    <h2>@Html.ActionLink("add new employee", "CreateView", "Employees")</h2>
    <table class="table">
        <tr>
            <th>Id</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Middle name</th>
            <th>Age</th>
            <th>Education</th>
            <th>Profession</th>
            <th></th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.MiddleName</td>
                <td>@item.Age</td>
                <td>@item.Education</td>
                <td>@item.Profession.Name</td>
                <td>@Html.ActionLink("change", "Update", "Employees", new { id=item.Id })</td>
                <td>@Html.ActionLink("delete", "Delete", "Employees", new { id=item.Id })</td>
            </tr>
        }
    </table>
</div>
